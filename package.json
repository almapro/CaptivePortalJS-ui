{
  "name": "captive-portal-js-ui",
  "description": "Captive Portal JS - UI",
  "author": "AlMA PRO",
  "version": "0.1.0-beta.9",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "5.0.5",
    "@mui/material": "5.0.6",
    "@mui/styles": "5.0.2",
    "@testing-library/jest-dom": "5.15.0",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.6",
    "@types/react": "17.0.34",
    "@types/react-dom": "17.0.11",
    "electron-compile": "^6.4.4",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "graphology-layout": "0.4.1",
    "graphology-layout-forceatlas2": "0.8.1",
    "lodash": "^4.17.21",
    "neo4j-driver": "4.3.5",
    "node-libs-browser": "^2.2.1",
    "notistack": "2.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6.0.1",
    "react-scripts": "4.0.3",
    "react-sigma-v2": "1.0.3",
    "react-use": "^17.3.1",
    "sigma": "2.0.0",
    "svg-to-dataurl": "^1.0.0",
    "uuid": "^8.3.2",
    "web-vitals": "2.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"BROWSER=none yarn start\" \"yarn electron\"",
    "electron": "wait-on tcp:3000 && electron-forge start -- --no-sandbox --remote-debugging-port=9222",
    "package": "react-scripts build && electron-forge package",
    "make": "react-scripts build && electron-forge make"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.61",
    "@types/electron": "^1.6.10",
    "@types/enzyme": "3.10.10",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/lodash": "4.14.176",
    "@types/react-router-dom": "5.3.2",
    "@types/uuid": "^8.3.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "6.3.0",
    "devtron": "^1.4.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "graphology": "0.22.1",
    "graphology-types": "0.21.1",
    "neo-forgery": "^2.0.0",
    "typescript": "^4.4.4",
    "wait-on": "^6.0.0"
  },
  "main": "./public/electron.js",
  "config": {
    "forge": {
      "makers": [
        {
          "name": "@electron-forge/maker-dmg",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "Authors": "AlMA PRO",
            "exe": "captive-portal-js-ui-${version}.exe"
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "options": {
              "maintainer": "AlMA PRO",
              "homepage": "https://github.com/almapro/CaptivePortalJS-ui"
            }
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {
            "options": {
              "maintainer": "AlMA PRO",
              "homepage": "https://github.com/almapro/CaptivePortalJS-ui"
            }
          }
        }
      ],
      "electronPackagerConfig": {
        "packageManager": "yarn",
        "asar": true
      },
      "github_repository": {
        "owner": "almapro",
        "name": "CaptivePortalJS-ui"
      }
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
